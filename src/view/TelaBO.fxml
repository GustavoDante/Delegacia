<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaBOController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <GridPane hgap="5.0" layoutX="25.0" layoutY="-17.0" prefHeight="417.0" prefWidth="641.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="pessoaFisica" mnemonicParsing="false" onAction="#trocaParaPF" prefHeight="84.0" prefWidth="235.0" text="PESSOA FÍSICA" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="TOP">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="pessoaJuridica" mnemonicParsing="false" onAction="#trocaParaPJ" prefHeight="84.0" prefWidth="235.0" text="PESSOA JURÍDICA" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="TOP">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="cancelar" mnemonicParsing="false" onAction="#trocarParaPrincipal" prefHeight="44.0" prefWidth="133.0" text="CANCELAR" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="BOTTOM" />
                  <Button fx:id="anonimo" layoutX="93.0" layoutY="149.0" mnemonicParsing="false" onAction="#trocaParaAnonimo" prefHeight="43.0" prefWidth="136.0" text="ANONIMO" GridPane.halignment="LEFT" GridPane.rowIndex="3" GridPane.valignment="BOTTOM">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
               </children>
            </GridPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
